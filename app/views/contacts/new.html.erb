<h1>New Contact</h1>

<%= form_for :contact, url: contacts_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :number %><br>
    <%= f.text_area :number %>
  </p>
 
  <p>
    <%= f.label :address %><br>
    <%= f.text_area :address %>
  </p>
  
  <p>
    <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>